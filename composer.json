{
    "type": "project",
    "name": "pmkr/pmkr",
    "description": "pmkr helps to compile and manage multiple PHP versions.",
    "keywords": [
        "php",
        "compiler"
    ],
    "authors": [
        {
            "name": "Andor",
            "role": "Maintainer",
            "homepage": "https://github.com/Sweetchuck"
        }
    ],
    "homepage": "https://github.com/Sweetchuck/pmkr",
    "support": {
        "source": "https://github.com/Sweetchuck/pmkr",
        "issues": "https://github.com/Sweetchuck/pmkr/issues"
    },
    "license": "GPL-3.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "cweagans/composer-patches": true,
            "sweetchuck/git-hooks": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "repositories": {
        "sweetchuck/env-var-storage": {
            "type": "path",
            "url": "../../sweetchuck/env-var-storage-1.x"
        },
        "sweetchuck/pear-client": {
            "type": "path",
            "url": "../../sweetchuck/pear-client-1.x"
        },
        "sweetchuck/robo-download-curl": {
            "type": "path",
            "url": "../../sweetchuck/robo-download-curl-1.x"
        },
        "sweetchuck/robo-git": {
            "type": "path",
            "url": "../../sweetchuck/robo-git-2.x"
        },
        "sweetchuck/robo-hash": {
            "type": "path",
            "url": "../../sweetchuck/robo-hash-2.x"
        }
    },
    "require": {
        "php": ">=7.4",
        "ext-bz2": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-phar": "*",
        "cebe/php-openapi": "dev-master#41db38d16ba157d4a1002b3087f43be3dabf9a2e",
        "composer/semver": "^3.2",
        "consolidation/annotated-command": "4.x-dev",
        "consolidation/config": "2.x-dev",
        "consolidation/robo": "3.x-dev",
        "cweagans/composer-patches": "^1.7",
        "opis/json-schema": "^2.2",
        "pear/archive_tar": "^1.4",
        "sweetchuck/env-var-storage": "1.x-dev",
        "sweetchuck/pear-client": "1.x-dev",
        "sweetchuck/robo-download-curl": "1.x-dev",
        "sweetchuck/robo-hash": "2.x-dev",
        "sweetchuck/utils": "1.x-dev",
        "symfony/filesystem": "^5.4",
        "symfony/finder": "^5.4",
        "symfony/yaml": "^5.3",
        "twig/twig": "^3.3"
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^2.0",
        "mikey179/vfsstream": "^1.6",
        "nuvoleweb/robo-config": "^2.0",
        "phpstan/phpstan": "^1.4",
        "squizlabs/php_codesniffer": "^3.6",
        "sweetchuck/codeception-module-robo-task-runner": "2.x-dev",
        "sweetchuck/git-hooks": "2.x-dev",
        "sweetchuck/robo-git": "2.x-dev",
        "sweetchuck/robo-phpcs": "2.x-dev",
        "sweetchuck/robo-phpmd": "2.x-dev",
        "sweetchuck/robo-phpstan": "1.x-dev",
        "symfony/var-dumper": "^5.4"
    },
    "bin": [
        "./bin/pmkr"
    ],
    "autoload": {
        "psr-4": {
            "Pmkr\\Pmkr\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pmkr\\Pmkr\\Tests\\Acceptance\\": "tests/acceptance/",
            "Pmkr\\Pmkr\\Tests\\Unit\\": "tests/unit/",
            "Pmkr\\Pmkr\\Tests\\": "tests/_support/"
        }
    },
    "scripts": {
        "clean": [
            "@clean:backend"
        ],
        "clean:backend": "rm --recursive --force ./vendor/"
    },
    "scripts-descriptions": {},
    "extra": {
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "patches": {
            "codeception/lib-asserts": {
                "https://github.com/Codeception/lib-asserts/issues/10 - Use FQN in PHPDoc type hints": "https://patch-diff.githubusercontent.com/raw/Codeception/lib-asserts/pull/11.patch"
            }
        }
    }
}
